<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}UnsapaIPW{% endblock %}</title>
        <link rel="icon" sizes="16x16" href="{{ asset('favicon.ico') }}" />
        {% stylesheets '@UnsapaIPWBundle/Resources/public/css/application.css' %}
          <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}

    </head>
    <body>
        {% block menu %}
        <nav>
        <a alt="Mes examens" href="{{ path('exams') }}">
          <img src="{{ asset('bundles/unsapaipw/images/documents.png') }}" width="75px" height="75px" alt="Mes examens" />
        </a>
        </nav>
        {% endblock %}

        {% block content %}
        {% endblock %}

        <div class="fos_user_content">
            {% block fos_user_content %}
            {% endblock %}
        </div>

    </body>
</html>
