
<form id="record_submit_form" action="{{ path('exam_submit') }}" method="post" {{ form_enctype(form) }}>
    {{ form_errors(form) }}

    {{ form_errors(form.exam) }}
    {{ form_label(form.exam) }}
    {{ form_widget(form.exam) }}
    <br>
    {{ form_errors(form.file) }}
    {{ form_label(form.file) }}
    {{ form_widget(form.file) }}
    (PDF, DOC, DOCX, ZIP)
    <br>
    {{ form_rest(form) }} 
    <input type="submit" />
</form>
